{
  "theme": "auto",
  "color_scheme": "Ros√© Pine.sublime-color-scheme",
  "font_face": "CaskaydiaMono Nerd Font",
  "font_size": 14,
  "font_options": ["subpixel_antialias"],
  "line_padding_bottom": 1,
  "line_padding_top": 1,
  "caret_style": "smooth",
  "highlight_line": true,
  "highlight_modified_tabs": true,
  "show_encoding": true,
  "show_line_endings": true,
  "translate_tabs_to_spaces": true,
  "tab_size": 2,
  "trim_trailing_white_space_on_save": true,
  "ensure_newline_at_eof_on_save": true,
  "save_on_focus_lost": true,
  "scroll_past_end": true,
  "word_wrap": false,
  "draw_white_space": ["selection", "trailing"],
  "folder_exclude_patterns": [".git", "node_modules", ".cache", ".next"],
  "file_exclude_patterns": ["*.pyc", "*.pyo", ".DS_Store"],
  "rulers": [80, 120]
}
